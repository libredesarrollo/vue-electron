{
  "name": "vueelectron",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "DEBUG=true electron .",
    "dev:macos": "electron-builder --macos --dir",
    "pro:macos": "electron-builder --macos"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bootstrap": "^5.2.2",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "electron-db": "^0.15.7",
    "express": "^4.18.1",
    "mysql": "^2.18.1",
    "vue": "^3.2.13",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "electron": "^20.1.3",
    "electron-builder": "^23.3.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
